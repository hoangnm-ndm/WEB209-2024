	"type": "module",
	"scripts": {
		"dev": "concurrently \"vite --port 5173\" \"json-server --watch db.json --port 3000 -m ./node_modules/json-server-auth\"",
		"server": "json-server --watch db.json --port 3000 -m ./node_modules/json-server-auth",
